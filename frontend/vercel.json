{
  "version": 2,
  "functions": {
    "api/leetcode-proxy.ts": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/leetcode-proxy/(.*)",
      "dest": "/api/leetcode-proxy",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}