{
  "version": 2,
  "functions": {
    "api/leetcode-proxy.ts": {
      "maxDuration": 10
    }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/leetcode-proxy/(.*)",
      "destination": "/api/leetcode-proxy"
    },
    {
      "source": "/((?!assets/).*)",
      "destination": "/index.html"
    }
  ]
}